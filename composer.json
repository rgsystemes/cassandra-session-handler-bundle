{
    "name": "alfatraining/cassandra-session-handler-bundle",
    "description": "Cassandra Session Handler Bundle for Symfony 4",
    "keywords": ["symfony", "bundle", "datastax", "cassandra", "session"],
    "type": "library",
    "require": {
        "php": ">=8.0.0",
        "ext-cassandra": ">=1.4.0",
        "datastax/php-driver": "^1.4",
        "symfony/framework-bundle": "^4.2",
        "psr/log": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lohse",
            "email": "daniel.lohse@alfatraining.de"
        }
    ],
    "minimum-stability": "stable",
    "config": {
      "optimize-autoloader": true
    },
    "autoload": {
      "classmap": ["Lib/"],
      "psr-4": { "Alfatraining\\CassandraSessionHandlerBundle\\": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/rgsystemes/php-driver"
        }
    ]
}
